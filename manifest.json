{
   "author" : "Ruslan Osmanov",
   "browser_specific_settings": {
       "gecko": {
           "id": "bee@ruslan_osmanov.com"
       }
   },
   "background" : {
      "service_worker": "js/eventPage.js",
      "type": "module"
   },
   "minimum_chrome_version": "92",
   "commands" : {
      "bee-editor" : {
         "description" : "__MSG_shortcutOpenEditor__",
         "suggested_key" : {
            "default" : "Ctrl+E",
            "mac" : "Command+E"
         }
      }
   },
   "default_locale" : "en",
   "description" : "__MSG_extDescription__",
   "icons" : {
      "128" : "img/icon128.png",
      "16" : "img/icon16.png",
      "48" : "img/icon48.png"
   },
   "manifest_version" : 3,
   "name" : "__MSG_extName__",
   "options_ui" : {
      "page" : "html/options.html"
   },
   "permissions" : [
      "activeTab",
      "storage",
      "tabs",
      "scripting",
      "nativeMessaging"
   ],
   "host_permissions": ["*://*/*"],
   "short_name" : "Bee",
   "version" : "3.2",
   "web_accessible_resources" : [{
       "resources": ["js/pattern.js", "js/storage.js"],
       "matches": [],
       "extension_ids": []
   }]
}
